package com.aurionpro.creational.factory.model;

public class CarFactory {

	private Icar icar;
	public Icar makeCar(String carName) {
		if(carName=="Maruti")
			icar= new Maruti();
		
		if(carName=="Tata")
			icar= new Tata();
		
		if(carName=="Mahindra")
			icar= new Mahindra();
		
		return icar;
		
	}
}
